{
  "name":"DMX Sequence Player",
  "type":"DMX",
  "path":"Protocol",
  
  "version":"1.0.0",
  "description":"Use a DMX universe to play sequences in chataigne. Each channel controls one sequence, 0 is stop, 1 is pause, then its volume.",
  "url":"https://github.com/norbertrostaing/dmxSequencePlayerChataigne",
  "downloadURL":"https://github.com/norbertrostaing/dmxSequencePlayerChataigne/archive/master.zip",

  "hasInput":true,
  "hasOutput":true, 

  "hideDefaultCommands":false,
  

  "defaults":
  {	
    "dmxType":"Art-Net",
    "artNet":
    {
      "input" : 
      {
        "enabled":true
      },
      "output" : 
      {
        "enabled":false
      }
    }
  },

  "scripts": 
  [
    "dmxSequencePlayer.js"
  ],

}
